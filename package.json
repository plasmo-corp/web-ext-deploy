{
  "name": "@plasmo-corp/web-ext-deploy",
  "version": "0.8.6",
  "description": "A library for deploying WebExtensions to multiple stores.",
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/plasmo-corp/web-ext-deploy"
  },
  "keywords": [
    "web-ext",
    "web-ext-deploy",
    "web-ext-publish",
    "chrome-deploy",
    "chrome-publish",
    "firefox-deploy",
    "firefox-publish",
    "opera-deploy",
    "opera-publish",
    "edge-deploy",
    "edge-publish"
  ],
  "files": [
    "dist/**/*"
  ],
  "author": "Louis <louis@plasmo.com>",
  "bugs": {
    "email": "support@plasmo.com",
    "url": "https://github.com/plasmo-corp/web-ext-deploy/issues"
  },
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/plasmo-corp/web-ext-deploy#readme",
  "scripts": {
    "debug:dev": "nodemon --config nodemon-dev.json",
    "debug:prod": "nodemon --",
    "clean": "rimraf dist",
    "prepare": "pnpm run clean && pnpm run build",
    "build": "tsc --build",
    "watch": "tsc --watch",
    "types:check": "tsc --noEmit",
    "format": "prettier --write \"{src,test,examples}/**/*.{ts,tsx}\"",
    "lint": "eslint \"{src,test,examples}/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"{src,test,examples}/**/*.{ts,tsx}\" --fix"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "pnpm run lint:fix",
      "pnpm run format",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/node": "^17.0.18",
    "@types/puppeteer": "^5.4.4",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.7.0-dev.20220217"
  },
  "dependencies": {
    "camel-case": "^4.1.2",
    "chrome-webstore-upload": "^1.0.0",
    "compare-versions": "^4.1.3",
    "dotenv": "^16.0.0",
    "parse-duration": "^1.0.2",
    "puppeteer": "^13.3.2",
    "zip-local": "^0.3.5"
  }
}
